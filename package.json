{
  "name": "event-ensure",
  "version": "1.0.0",
  "description": "RSVP ensurance app",
  "private": true,
  "main": "server.js",
  "author": "",
  "license": "ISC",
  "scripts": {
    "precommit": "lint-staged",
    "build": "webpack",
    "dev": "concurrently -k \"npm run frontend\" \"npm run backend\"",
    "frontend": "webpack-dev-server",
    "backend": "nodemon server.js",
    "deploy": "./scripts/deploy.sh",
    "test":
      "mocha --compilers js:babel-core/register ./test/**/*.js --require ignore-styles"
  },
  "babel": {
    "presets": ["env", "react"],
    "plugins": [
      "transform-object-rest-spread",
      "react-hot-loader/babel",
      [
        "transform-runtime",
        {
          "polyfill": false,
          "regenerator": true
        }
      ]
    ]
  },
  "nodemonConfig": {
    "ignore": ["frontend/**", "public/**"]
  },
  "lint-staged": {
    "**/*.{js,jsx,json,css}": ["prettier --single-quote --write", "git add"]
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "bootstrap": "^4.0.0",
    "cookie-session": "^2.0.0-beta.3",
    "express": "^4.15.2",
    "mongoose": "^5.0.6",
    "node-sass": "^4.5.2",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "prop-types": "^15.5.8",
    "qs": "^6.5.1",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-hot-loader": "^4.0.0",
    "react-redux": "^5.0.5",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-transition-group": "^2.2.1",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.2",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.5.0",
    "css-loader": "^0.28.0",
    "enzyme": "^2.8.1",
    "eslint": "^3.19.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "husky": "^0.14.3",
    "ignore-styles": "^5.0.1",
    "lint-staged": "^7.0.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "prettier": "^1.11.1",
    "react-addons-test-utils": "^15.5.1",
    "react-test-renderer": "^15.5.4",
    "redux-devtools": "^3.4.0",
    "redux-devtools-dock-monitor": "^1.1.2",
    "redux-devtools-log-monitor": "^1.3.0",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "url-loader": "^1.0.1",
    "webpack": "^2.3.3",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-dev-server": "^2.4.2",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-merge": "^4.1.2"
  }
}
